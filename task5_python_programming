\")\n","for student in students:\n","    print(student)\n","bubble_sort_scores(students)\n","print(\"\\nAfter sorting:\")\n","for student in students:\n","    print(student)\n"],"metadata":{"id":"51rR1mUV-dTu","executionInfo":{"status":"ok","timestamp":1756186273236,"user_tz":-330,"elapsed":16,"user":{"displayName":"Sumanth Binginapalli","userId":"15429370894417438195"}},"outputId":"52358b08-3b62-487f-d53b-6c805d7a6181","colab":{"base_uri":"https://localhost:8080/"}},"execution_count":3,"outputs":[{"output_type":"stream","name":"stdout","text":["Before sorting:\n","{'name': 'Alice', 'score': 88}\n","{'name': 'Bob', 'score': 95}\n","{'name': 'Charlie', 'score': 75}\n","{'name': 'Diana', 'score': 85}\n","\n","After sorting:\n","{'name': 'Charlie', 'score': 75}\n","{'name': 'Diana', 'score': 85}\n","{'name': 'Alice', 'score': 88}\n","{'name': 'Bob', 'score': 95}\n"]}]}]}